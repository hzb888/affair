{
    "pages": [
        "pages/index/index",
        "pages/me/me",
        "pages/map/map",
        "pages/schdeuel/schdeuel",
        "pages/login/login",
        "pages/club/club",
        "pages/notice/notice",
        "pages/about/about",
        "pages/club_detail/club_detail"
    ],
    "subPackages": [],
    "window": {
        "navigationBarTitleText": "uni-app",
        "backgroundColor": "@backgroundColor",
        "backgroundTextStyle": "@backgroundTextStyle",
        "navigationBarBackgroundColor": "@navBackgroundColor",
        "navigationBarTextStyle": "@navTextStyle"
    },
    "tabBar": {
        "color": "@color",
        "backgroundColor": "@tabBackgroundColor",
        "selectedColor": "#6eaddf",
        "list": [
            {
                "pagePath": "pages/index/index",
                "iconPath": "static/tabbar/主页.png",
                "selectedIconPath": "static/tabbar/主页_sel.png",
                "text": "首页"
            },
            {
                "pagePath": "pages/schdeuel/schdeuel",
                "iconPath": "static/tabbar/课程表.png",
                "selectedIconPath": "static/tabbar/课程表_sel.png",
                "text": "课表"
            },
            {
                "pagePath": "pages/me/me",
                "iconPath": "static/tabbar/作者.png",
                "selectedIconPath": "static/tabbar/作者_sel.png",
                "text": "我的"
            }
        ]
    },
    "darkmode": true,
    "themeLocation": "theme.json",
    "lazyCodeLoading": "requiredComponents",
    "requiredPrivateInfos": [
        "getLocation",
        "onLocationChange",
        "startLocationUpdate"
    ],
    "permission": {
        "scope.userLocation": {
            "desc": "你的位置信息将用于小程序展示校内导航"
        }
    },
    "usingComponents": {},
    "sitemapLocation": "sitemap.json"
